class Main {
  public static void main(String[] args) {
    System.out.println("Shapes !");
    square(5); 
  }
  public static void square(int s) {
    for(int row = 3; row < s; row++){
      for(int col = 0; col < s; col++){
        System.out.print("* "); 
      } 
      System.out.println();
    }
  }
}
